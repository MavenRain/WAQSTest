    private Type _type;
    public Type Type
    {
        get { return _type; }
        private set 
        { 
            _type = value;
            value = GetType(value);
            GenericArguments = value.GetGenericArguments().Select(a => new SerializableType(a)).ToList();
            TypeName = Regex.Replace(value.Name, @"`(\d+)$", m =>

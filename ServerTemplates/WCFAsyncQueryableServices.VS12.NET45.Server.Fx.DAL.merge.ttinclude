            value = await baseSaveChangesAsync(options);
        }
        catch (OptimisticConcurrencyException e)
        {
            optimisticConcurrencyException = e;
        }
        catch (Exception e)
        {
            HandleOrReThrow(e);
        }
        if (optimisticConcurrencyException != null)
        {
            using (var contextTmp = new T())
